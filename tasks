#include <stdio.h>

enum Seasons {
    SPRING,
    SUMMER,
    FALL,
    WINTER
};

int main() {
    enum Seasons currentSeason = SUMMER;

    printf("Value of SPRING: %d\n", SPRING);
    printf("Value of SUMMER: %d\n", SUMMER);
    printf("Value of FALL: %d\n", FALL);
    printf("Value of WINTER: %d\n", WINTER);

    printf("Current season: %d\n", currentSeason);

    return 0;
}
