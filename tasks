#include <stdio.h>

union IntFloatUnion {
    int intValue;
    float floatValue;
};

int main() {
    union IntFloatUnion u;

    u.floatValue = 3.14f;
    printf("Float value: %f\n", u.floatValue);
    printf("Int value: %d\n", u.intValue);

    u.intValue = 42;
    printf("Int value: %d\n", u.intValue);
    printf("Float value: %f\n", u.floatValue);

    return 0;
}
