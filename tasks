#include <stdio.h>

struct BitfieldStruct {
    unsigned int a : 3;
    unsigned int b : 3;
    unsigned int c : 2;
};

int main() {
    struct BitfieldStruct myStruct;

    myStruct.a = 5;
    myStruct.b = 3;
    myStruct.c = 2;

    printf("%u\n", myStruct.a);
    printf("%u\n", myStruct.b);
    printf("%u\n", myStruct.c);

    return 0;
}
